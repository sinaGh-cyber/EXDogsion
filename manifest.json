{
  "manifest_version": 2,
  "name": "chrom ExDogysion",
  "version": "1.0.0",
  "web_accessible_resources": ["dogs/*.jpg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
